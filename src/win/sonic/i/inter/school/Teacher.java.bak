public class Teacher extends Identity implements IEat, IStay {

	@Override
	public void whereStay() {
		System.out.println("住在" + IStay.apartment + "\n");
	}

	@Override
	public void WhereEat() {
		System.out.println("在" + IEat.mess + "吃\n");

	}

	@Override
	public void WhatEat() {
		System.out.println("你可以吃" + IEat.mo + " 或者 " + IEat.meatWithMo + " 或者 " + IEat.porridge + ".\n");

	}

}
